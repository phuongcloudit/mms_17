<%= form_for [:admin, @team], url:  add_member_admin_team_path do |f| %>
  <%= render "shared/error_messages", object: f.object %>
  <div class="form-group col-md-9">
    <%= collection_select :team, :user_ids, @users, :id, :name, {},
  {class: "form-control chosen-select", include_hidden: false, multiple: true, data: { placeholder: t(".choose_members") }} %>
  </div>
  <div class="form-group col-md-3">
    <%= f.submit t(".update") ,class: "btn btn-success btn-block" %>
  </div>
<% end %>
